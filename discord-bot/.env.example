# Discord Bot Environment Variables - Level 10
# REQUIRED: Get from https://discord.com/developers/applications

# Bot Token (REQUIRED)
DISCORD_BOT_TOKEN=your_bot_token_here

# Application Configuration (REQUIRED)
CLIENT_ID=your_application_id_here
GUILD_ID=your_server_id_here

# Channel IDs (Update after bot joins server)
CHANGELOG_CHANNEL_ID=
BUILD_LOG_CHANNEL_ID=
WELCOME_CHANNEL_ID=
LOGS_CHANNEL_ID=

# Role IDs for XP-based assignment (Optional)
ROLE_ROOKIE_ID=
ROLE_VETERAN_ID=
ROLE_CHAMPION_ID=
ROLE_ELITE_ID=

# GG LOOP API Integration
GG_LOOP_API_URL=https://ggloop.io/api

# Environment
NODE_ENV=production
